<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_perfil"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alexj.negocios.Perfil">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/land"
        android:id="@+id/imageView7"
        android:layout_marginBottom="253dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar"
        android:layout_below="@+id/editText2"
        android:layout_alignEnd="@+id/editText2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText"
        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed ut tortor et eros semper porttitor nec interdum dui. In sit amet nisi eleifend, posuere purus vitae, convallis tortor"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/ratingBar"
        android:layout_below="@+id/ratingBar" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Puerta del sol"
        android:ems="10"
        android:layout_marginStart="18dp"
        android:id="@+id/editText2"
        android:layout_below="@+id/imageView9"
        android:layout_toEndOf="@+id/textView2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageView9"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Negocio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_marginStart="30dp"
        android:layout_alignBaseline="@+id/editText2"
        android:layout_alignBottom="@+id/editText2"
        android:layout_alignParentStart="true" />

</RelativeLayout>
